# Backend deployment environment variables
# Copy to .env and fill in values

# Optional: PostgreSQL (not required for hackathon demo)
# DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/dbname

# Optional: Redis (not required for hackathon demo)
# REDIS_URL=redis://host:6379/0

# Frontend URL for CORS (set to your Vercel deployment URL)
FRONTEND_URL=https://your-app.vercel.app

# LLM API keys (for coaching features)
ASI1_API_KEY=
ANTHROPIC_API_KEY=

# GRID API (for data extraction - not needed at runtime)
# GRID_API_KEY=

# Cloud Run sets this automatically
# PORT=8080
